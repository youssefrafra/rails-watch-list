<div class="container">
    <h1><%= @list.name %></h1>
    <br/>
    <%= simple_form_for [@list,@bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit "Add movie", class:"btn btn-success"%>
    <% end %>
    <br/>
    <%= link_to  "Back", list_path(@list), class:"btn btn-dark" %>
</div>